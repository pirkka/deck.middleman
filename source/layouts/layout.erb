<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
<script type="text/javascript">
	google.maps.visualRefresh = true;
	var styleArray = [
	  {
	    featureType: "all",
	    stylers: [
	      { saturation: -100 }
	    ]
	  },
	  {
	    featureType: "poi",
	    elementType: "labels",
	    stylers: [
	          { visibility: "off" }
	      ]
		},
		{
			featureType: "transit",
			stylers: [
	          { visibility: "off" }
	      ]
		}
	
	];
	
	var deckMapType = new google.maps.StyledMapType(styleArray,{ name: "Deck" });

	var lat = 60.158381;
	var lng = 24.933919;

	var map = new google.maps.Map(document.getElementById('deck-map'), {
	  zoom: 14,
	  center: new google.maps.LatLng(lat,lng),
	  mapTypeId: google.maps.MapTypeId.ROADMAP,
	  backgroundColor: "#FFF",
	  minZoom: 12,
	  maxZoom: 18,
	  scrollwheel: false,
	  streetViewControl: false,
	  mapTypeControl: false,
	  zoomControlOptions: {
	    position: google.maps.ControlPosition.LEFT_BOTTOM
	  },
	  panControlOptions: {
	    position: google.maps.ControlPosition.LEFT_BOTTOM	  
	  }
	});

	map.panBy(0,-60);
	
	var	marker = new google.maps.Marker({
		map: map,
		position: new google.maps.LatLng(lat, lng),
		icon: "/images/arrow-point2.png"
	});

	map.mapTypes.set('deck', deckMapType);
	map.setMapTypeId('deck');
</script>
	
